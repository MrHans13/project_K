                                name: "tab_gui"
                                id: tab_one
                                text: "Guides"
                                on_release:
                                    app.set_app_data("act_cat", "gui")
                                    app.set_app_data("act_scr", "gui_welcome")
                                    app.change_win("scr_two", "right")
                    BoxBgApp:
                        size_hint_x: 0.2
                        padding: 3,0,0,3
                        BoxBgTit:
                            Tab:
                                name: "tab_api"
