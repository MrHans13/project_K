<ScrOne,ScrTwo>:
    BoxBgApp:
        orientation: "vertical"
        padding: 4
        BoxBgTit:
            name: "titlebox"
            id: box_title
            orientation: "vertical"
            padding: 4
            size_hint_y: 0.08
            BoxBgApp:
                orientation: "horizontal"
                padding: 3,3,3,0
                BoxBgTit:
                    BoxBgTit:
                    BoxBgApp:
                        size_hint_x: 0.2
                        padding: 3,0,0,3
                        BoxBgTit:
