                                text: "API-Ref"
                                on_release:
                                    app.set_app_data("act_cat", "api")
                                    app.set_app_data("act_scr", "api_kivy")
                                    app.change_win("scr_one", "left")
            BoxBgApp:
                orientation: "horizontal"
                padding: 3,0,3,3
                BoxBgTit:
                    size_hint_x: 0.2
